{
  "name": "Ad Blocker",
  "version": "1.7",
  "description": "A simple ad blocker",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/", "activeTab"],
  "background": {
    "scripts": ["blocked_domains.js", "background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["ads.css"]
    }
  ]
}
